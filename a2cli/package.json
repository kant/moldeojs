{
  "name": "MoldeoJS",
  "version": "0.0.1",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build && cp -R src/molrepos dist/molrepos && cp -R src/data dist/data",
    "prepare": "cp -a electron/. dist/",
    "electron": "npm run prepare && electron ./dist",
    "package": "npm run build && npm run prepare && ./node_modules/electron-packager/cli.js dist App --platform=linux --arch=all --out=dist-app --overwrite",
    "buildelectron": "npm run build && npm run electron"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "0.0.43",
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/webrtc": "0.0.22",
    "core-js": "^2.4.1",
    "mathjs": "^3.9.1",
    "node-osc": "^2.0.4",
    "rxjs": "^5.5.6",
    "three": "^0.83.0",
    "ts-helpers": "^1.1.2",
    "xml2js": "^0.4.17",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "2.4.2",
    "@types/jasmine": "^2.5.41",
    "@types/mathjs": "0.0.33",
    "@types/node": "^7.0.0",
    "@types/three": "^0.81.0",
    "codelyzer": "^4.0.2",
    "electron": "^1.4.15",
    "electron-packager": "^8.5.1",
    "jasmine": "^2.5.2",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.1.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "^5.9.1",
    "typescript": "2.5.3",
    "webdriver-manager": "11.1.1"
  }
}
