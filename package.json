  "scripts": {
    "start": "ng serve --port 8888",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build",
    "prepare:electron": "",
    "start:electron": "npm run prepare:electron && electron .",
    "build:package": "npm run build && npm run prepare:electron && ./node_modules/electron-packager/cli.js dist App --platform=linux --arch=all --out=dist-app --overwrite",
    "build:electron": "ng build --base-href && npm run prepare:electron",
    "build:site": "npm version patch && ng build --prod --base-href .",
    "build:siteopt": "ng build --prod --build-optimizer --base-href . ",
    "build:odoo": "ng build --base-href / && cp -R src/assets/molrepos dist/molrepos && cp -R src/assets/data dist/data"
  },
  "private": true,
